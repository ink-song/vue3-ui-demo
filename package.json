    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "nprogress": "^0.2.0",
    "prettier": "^2.4.1",
