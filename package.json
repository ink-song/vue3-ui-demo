{
  "name": "vue3-ui-demo",
  "version": "0.2.7",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/undefined/**/*.json\"",
    "ct": "git add . && cz-customizable && standard-version -p gitmoji-config -i CHANGELOG.md -s",
    "test": "your-cmd",
    "prepare": "husky install",
    "changelog": "standard-version -p gitmoji-config -i CHANGELOG.md -s",
    "release": "standard-version --preset gitmoji-config"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.3",
    "commitizen": "^4.2.4",
    "core-js": "^3.6.5",
    "git-cz": "^4.9.0",
    "release": "^6.3.1",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "12.1.4",
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-plugin-typescript": "~4.5.12",
    "@vue/cli-plugin-vuex": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "axios": "^0.18.0",
    "commitlint": "^17.0.1",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "commitlint-config-gitmoji": "^2.2.5",
    "conventional-changelog-gitmoji-config": "^1.4.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "nprogress": "^0.2.0",
    "prettier": "^2.4.1",
    "qiankun": "^2.7.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.5.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-axios": "^0.0.4",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-cli-plugin-i18n": "^2.3.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
